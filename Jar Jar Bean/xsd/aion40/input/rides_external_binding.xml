<jxb:bindings version="1.0" xmlns:jxb="http://java.sun.com/xml/ns/jaxb" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <jxb:bindings schemaLocation="rides0.xsd" node="/xs:schema">
	
	<jxb:bindings node="//xs:element[@name='client_ride_datas']">
      <jxb:class name="ClientRides"/>
    </jxb:bindings>
	
	<jxb:bindings node="//xs:element[@name='client_ride_data']">
      <jxb:class name="ClientRide"/>
	  <!-- <jxb:property name="ClientRide"/> -->
    </jxb:bindings>
	
	<jxb:bindings node="//xs:element[@name='start_fp']">
		<jxb:javaType name="int" parseMethod="javax.xml.bind.DatatypeConverter.parseInt" printMethod="javax.xml.bind.DatatypeConverter.printInt"/>
    </jxb:bindings>
	
	<jxb:bindings node="//xs:element[@name='cost_fp']">
		<jxb:javaType name="int" parseMethod="javax.xml.bind.DatatypeConverter.parseInt" printMethod="javax.xml.bind.DatatypeConverter.printInt"/>
    </jxb:bindings>
	
	<jxb:bindings node="//xs:element[@name='ride_type']">
		<jxb:javaType name="int" parseMethod="javax.xml.bind.DatatypeConverter.parseInt" printMethod="javax.xml.bind.DatatypeConverter.printInt"/>
    </jxb:bindings>

  </jxb:bindings>
</jxb:bindings>